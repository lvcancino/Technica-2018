{
  "name": "SmartSurfing",
  "version": "1.0",
  "description": "Chrome Extension for Browsing Data Management",
  "permissions": ["history", "activeTab", "declarativeContent", "storage", "tabs"],
  "content_security_policy": "script-src 'self' https://www.gstatic.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  },
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "manifest_version": 2
}